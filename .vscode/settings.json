    "adblocker",
    "akka-persistence-jdbc",
    "akka-serialization-jackson",
    "akka-streams-kafka",
    "doobie",
    "freshrss",
    "hikaricp",
    "jackson-module-scala",
    "kafka-avro-serializer",
    "mima",
    "reeder",
    "scala-xml",
    "timtowtdi"
